{
  "name": "hacker-rank-review-aws",
  "bin": "bin/HackerRackQueue.js",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "compile": "tsc",
    "test": "jest",
    "cdk": "cdk",
    "infra:up": "cdk deploy --require-approval never",
    "infra:down": "cdk destroy --require-approval never",
    "deploy": "./scripts/deploy-dev.sh",
    "infra:up:prod": "node confirm.js && cdk deploy -c mode=prod --require-approval never",
    "infra:down:prod": "node confirm.js && cdk destroy -c mode=prod --require-approval never"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.108.0",
    "@types/jest": "^26.0.10",
    "@types/node": "^10.17.27",
    "aws-cdk": "1.108.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-ec2": "1.108.0",
    "@aws-cdk/aws-ecr": "1.108.0",
    "@aws-cdk/aws-ecs": "1.108.0",
    "@aws-cdk/aws-ecs-patterns": "1.108.0",
    "@aws-cdk/aws-route53": "1.108.0",
    "@aws-cdk/aws-secretsmanager": "1.108.0",
    "@aws-cdk/core": "1.108.0",
    "source-map-support": "^0.5.16"
  }
}
