    "jest": "29.3.1",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@types/tar": "6.1.3",
    "aws-cdk-lib": "2.61.1",
    "constructs": "10.1.228",
    "source-map-support": "0.5.21",
    "tar": "6.1.13"
  }
}
