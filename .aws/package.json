{
  "name": "hacker-rank-review-aws",
  "bin": "bin/HackerRackQueue.js",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "compile": "tsc",
    "test": "jest",
    "cdk": "cdk",
