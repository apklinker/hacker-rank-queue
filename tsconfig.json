{
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@utils/*": ["src/utils/*"],
      "@bot/*": ["src/bot/*"],
      "@models/*": ["src/database/models/*"],
      "@repos/*": ["src/database/repos/*"],
      "@database": ["src/database/database"],
      "@/*": ["src/*"]
    }
  }
}
