{
  "name": "hacker-rank-queue",
  "version": "0.2.0",
  "description": "Slack bot for managing a queue of reviewers for HackerRanks",
  "main": "index.js",
  "repository": "https://github.com/apklinker/hacker-rank-queue",
  "author": "Source Allies",
  "license": "MIT",
  "private": true,
  "workspaces": [
    ".",
    ".aws"
  ],
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "yarn build && node --enable-source-maps dist/index.js",
    "compile": "tsc -p . --noEmit",
    "build": "rollup -c rollup.config.js",
    "build:docker": "docker build -t sai/hacker-rank-queue -f docker/Dockerfile .",
    "lint": "yarn lint:no-fix --fix",
    "lint:no-fix": "eslint . --ext .js,.ts --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "verify": "yarn lint:no-fix && yarn format:check && yarn compile && yarn build && yarn test",
    "deploy": "cd .aws && ./scripts/deploy-dev.sh ; cd ..",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/chance": "^1.1.2",
    "@types/google-spreadsheet": "^3.0.2",
    "@types/jest": "^26.0.23",
    "@types/node": "~14",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "chance": "^1.1.7",
    "eslint": "^7.23.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "rollup": "^2.44.0",
    "rollup-plugin-typescript-paths": "^1.2.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@slack/bolt": "^3.3.0",
    "dotenv": "^8.2.0",
    "google-spreadsheet": "^3.1.15"
  }
}
