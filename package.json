{
  "name": "hacker-rank-queue",
  "version": "0.2.0",
  "description": "Slack bot for managing a queue of reviewers for HackerRanks",
  "main": "index.js",
  "repository": "https://github.com/apklinker/hacker-rank-queue",
  "author": "Source Allies",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "start": "pnpm build && node --enable-source-maps dist/index.js",
    "compile": "tsc -p . --noEmit",
    "build": "tsup src/index.ts --target node14 --sourcemap",
    "build:docker": "docker build -t sai/hacker-rank-queue -f docker/Dockerfile .",
    "lint": "pnpm lint:no-fix --fix",
    "lint:no-fix": "eslint . --ext .js,.ts --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "verify": "pnpm lint:no-fix && pnpm format:check && pnpm compile && pnpm build && pnpm test",
    "deploy": "cd .aws && ./scripts/deploy-dev.sh ; cd ..",
    "postinstall": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@types/chance": "1.1.3",
    "@types/google-spreadsheet": "3.1.5",
    "@types/jest": "27.4.0",
    "@types/node": "14.18.5",
    "@types/node-cron": "2.0.5",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "chance": "1.1.8",
    "eslint": "8.7.0",
    "eslint-plugin-only-warn": "1.0.3",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "ts-jest": "27.1.3",
    "ts-node": "9.1.1",
    "tslib": "2.3.1",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "@slack/bolt": "3.8.1",
    "dotenv": "8.6.0",
    "google-spreadsheet": "3.2.0",
    "lock": "1.1.0",
    "node-cron": "3.0.0",
    "tsup": "4.14.0"
  }
}
